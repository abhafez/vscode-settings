{
  "markdown.preview.fontFamily": "'TerminessTTF Nerd Font Mono','TerminessTTFNerdFontMono'",
  "markdown.preview.fontSize": 20,
  // "editor.formatOnPaste": true,
  // files
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 0,
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "files.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.git": false,
    "**/.DS_Store": true,
    "**/.rpt2_cache": true,
    "node_modules": false,
    "test-lib": false,
    "lib": false,
    "dist": false,
    "coverage": false,
    "npm": false
  },
  "files.trimTrailingWhitespace": false,
  "files.insertFinalNewline": false,
  // emmet
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {"HTML (Eex)": "html"},
  //
  //  others
  "trailing-spaces.trimOnSave": false,
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.symbolPath": "on",
  "breadcrumbs.symbolSortOrder": "type",
  // gitlens
  "gitlens.hovers.enabled": false,

  //   window
  // "window.title": "${dirty}${rootName}${separatpr}${activeEditorShort}${separator}",
  "window.openFoldersInNewWindow": "default",
  "window.menuBarVisibility": "hidden",
  "window.zoomLevel": -1,
  //  extensions
  "extensions.autoCheckUpdates": true,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "extensions.showRecommendationsOnlyOnDemand": true,
  //  zen mode
  "zenMode.restore": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.fullScreen": false,
  "zenMode.centerLayout": false,
  "zenMode.hideActivityBar": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  //  terminal 
  "terminal.integrated.fontWeightBold": "900",
  "terminal.integrated.fontFamily": "'TerminessTTF Nerd Font Mono','TerminessTTFNerdFontMono'",
  "terminal.integrated.fontSize": 20,
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "terminal.integrated.shellArgs.linux": ["-l"],
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.cursorBlinking": true,
  "terminal.external.linuxExec": "st",
  "terminal.integrated.lineHeight": 0.5,
  //  Code Runner 
  "code-runner.saveFileBeforeRun": false,
  "code-runner.preserveFocus": false,
  "code-runner.terminalRoot": "zsh",
  // Languages settings
  //  javascript 
  "javascript.suggest.autoImports": true,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "javascript.format.enable": false,
  "javascript.preferences.quoteStyle": "single",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [100]
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.rulers": [100]
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  // eslint & prettier
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": false
    },
    {
      "language": "html",
      "autoFix": false
    },
    {
      "language": "javascript",
      "autoFix": false
    },
    {
      "language": "javascriptreact",
      "autoFix": false
    },
    {
      "language": "typescript",
      "autoFix": false
    }
  ],
  "jest.autoEnable": false,
  // npm
  "npm-intellisense.importES6": false,
  //  html 
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.rulers": [100]
  },
  //  elixir 
  "[elixir]": {
    "editor.acceptSuggestionOnEnter": "on",
    "editor.rulers": [80],
  },
  //  ruby 
  "ruby.intellisense": "rubyLocate",
  // "solargraph.commandPath": "/home/hafez/.rbenv/shims/solargraph",
  "ruby.codeCompletion": "rcodetools",
  "ruby.rctComplete.commandPath": "/home/hafez/.rbenv/shims/rct-complete",
  "ruby.rubocop.executePath": "/home/hafez/.rbenv/shims/",
  "ruby.rubocop.onSave": true,
  "ruby.useBundler": true,
  // ﭱ cpp ﭱ
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.autocomplete": "Default",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "C_Cpp.default.compilerPath": "/usr/bin/gcc",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.updateChannel": "Insiders",
  "[cpp]": {
    "editor.tabSize": 4
  },
  //  Python 
  "python.jediEnabled": false,
  "python.globalModuleInstallation": true,
  // "python.formatting.autopep8Path": "${workspaceFolder}/env/bin/",
  "python.formatting.autopep8Path": "/usr/bin/autopep8",
  "python.pythonPath": "python3",
  "python.autoUpdateLanguageServer": false,
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.workspaceSymbols.rebuildOnStart": false,
  "python.pipenvPath": "${workspaceFolder}/env",
  "[python]": {
    "editor.tabSize": 4,
    "editor.rulers": [80]
  },
  //  json
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //bracket colorizer
  "bracketPairColorizer.consecutivePairColors": [
    ["{", "}"],
    ["(", ")"],
    ["[", "]"],
    ["<", ">"],
    ["</", ">"],
    ["#dcdc8b", "#94ebb1", "#A4FFFF"],
    "Red"
  ],
  // workbench
  "workbench.activityBar.visible": false,
  "workbench.editor.tabCloseButton": "right",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.colorCustomizations": {
