{
  // Window
  "window.menuBarVisibility": "hidden",
  // Breadcrumbs
  "breadcrumbs.symbolSortOrder": "position",
  // workbench
  "workbench.iconTheme": "helium-icon-theme",
  "workbench.sideBar.location": "right",
  "workbench.settings.editor": "json",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.productIconTheme": "fluent-icons",
  // Fonts
  "editor.fontWeight": "400",
  "terminal.integrated.fontSize": 17,
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Rec Mono Duotone2', 'JetBrains Mono', 'Overpass Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "scm.inputFontFamily": "'Rec Mono Duotone2', 'JetBrains Mono'",
  "editor.inlineHints.fontFamily": "'Rec Mono Duotone2', 'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.fontFamily": "'Rec Mono Duotone2', 'JetBrains Mono', 'Overpass Mono', 'NotoSansMono Nerd Font'",
  // Editor
  "editor.tabSize": 2,
  "editor.tabCompletion": "on",
  "editor.mouseWheelZoom": true,
  "editor.snippetSuggestions": "bottom",
  "emmet.showSuggestionsAsSnippets": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.minimap.enabled": false,
  // editor find/Search
  "editor.find.autoFindInSelection": "multiline",
  "editor.find.cursorMoveOnType": true,
  "editor.find.seedSearchStringFromSelection": true,
  "editor.parameterHints.enabled": false,
  // Debug
  "debug.toolBarLocation": "docked",
  // TODO
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.filtering.excludeGlobs": ["node_modules"],
  "todo-tree.general.enableFileWatcher": true,
  "todo-tree.general.statusBar": "total",
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "line",
  "todo-tree.general.tags": [
    "BUG",
    "NOTE:",
    "FIXME",
    "TODO",
    "CONFIRM",
    "ASK",
    "[ ]",
    "[x]"
  ],
  "todo-tree.general.tagGroups": {
    "FIXME": ["FIXME", "FIXIT", "FIX"]
  },
  "todo-tree.highlights.defaultHighlight": {
    "icon": "list",
    "type": "text",
    "opacity": 50,
    "foreground": "white",
    "iconColour": "#8F8A27",
    "background": "#8F8A27"
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check"
    },
    "CONFIRM": {
      "icon": "check",
      "background": "#8F8A27"
    },
    "NOTE:": {
      "icon": "note",
      "background": "#D28520"
    },
    "BUG": {
      "icon": "bug",
      "background": "#B62330"
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // FILE ASSOCIATION
  "files.associations": {
    "*.js": "javascript"
  },
  // Highlight matching tag
  "highlight-matching-tag.styles": {
    "opening": {
      "left": {
        "custom": {
          "borderWidth": "3px 0 3px 3px",
          "borderStyle": "solid",
          "borderColor": "#d79921",
          "borderRadius": "2px"
        }
      },
      "right": {
        "custom": {
          "borderWidth": "3px 3px 3px 0",
          "borderStyle": "solid",
          "borderColor": "#d79921",
          "borderRadius": "2px"
        }
      }
    }
  },
  "bracketPairColorizer.consecutivePairColors": [
    ["{", "}"],
    ["(", ")"],
    ["[", "]"],
    ["#EBCB8B", "#d79921", "#ce9887"],
    "Red"
  ],
  // NODE
  "node-module-intellisense.scanBuiltinModules": true,
  "node-module-intellisense.scanDevDependencies": true,
  "node-module-intellisense.scanFileModules": true,
  "node-module-intellisense.modulePaths": [],
  "node-module-intellisense.autoStripExtensions": [
    ".js",
    ".jsx",
    ".ts",
    ".d.ts",
    ".tsx"
  ],
  /**                                                                                             ✔ 
     _   _ _   _ ___ _____    _____ _____ ____ _____ 
    | | | | \ | |_ _|_   _|  |_   _| ____/ ___|_   _|
    | | | |  \| || |  | |     | | |  _| \___ \ | |  
    | |_| | |\  || |  | |     | | | |___ ___) || |  
    \___/|_| \_|___|  |_|     |_| |_____|____/ |_|  
                                                  
  */
  "createTestFile.nameTemplate": "__tests__/{filename}.test",
  "createTestFile.languages": {
    "[javascript]": {
      "createTestFile.nameTemplate": "__tests__/{filename}.test"
    }
  },
  "createTestFile.pathMaps": [
    {
      // Defines rule such that any file under app/ will have a test file created under test/
      // Rules will be applied in the order they are defined. The first rule to match the file path will be used.
      "pathPattern": "__tests__(/.*)?", // Regex file path matcher
      "testFilePath": "__tests__$1" // $1, $2, etc. will be replaced with the matching text from the pathPattern
    }
  ],
  "git.autofetch": true,
  /* 
            _                                _       _   
          (_) __ ___   ____ _ ___  ___ _ __(_)_ __ | |_ 
          | |/ _` \ \ / / _` / __|/ __| '__| | '_ \| __|
          | | (_| |\ V / (_| \__ \ (__| |  | | |_) | |_ 
        _/ |\__,_| \_/ \__,_|___/\___|_|  |_| .__/ \__|
        |__/                                 |_|        
          
  */
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "js/ts.implicitProjectConfig.checkJs": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.gitAuthentication": false,

