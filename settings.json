{
  "window.menuBarVisibility": "hidden",
  "window.enableMenuBarMnemonics": false,
  "window.customMenuBarAltFocus": false,
  "window.titleSeparator": " ðŸ—» ",
  "editor.autoClosingBrackets": "never",
  "window.title": "${dirty} ${separator} ${rootName}",
  "editor.bracketPairColorization.enabled": true,
  "editor.matchBrackets": "always",
  "window.titleBarStyle": "custom",
  "editor.guides.bracketPairs": "active",
  "workbench.colorTheme": "Catppuccin Mocha",
  "catppuccin.bracketMode": "neovim",
  "catppuccin.italicKeywords": false,
  "catppuccin.italicComments": false,
  "catppuccin.accentColor": "sky",
  "catppuccin.boldKeywords": true,
  "catppuccin.workbenchMode": "flat",
  "catppuccin.extraBordersEnabled": true,
  "material-icon-theme.activeIconPack": "nest",
  // EDITOR
  "editor.fontFamily": "'Hasubi Mono', 'SpaceMono Nerd Font Mono', 'Victor Mono', 'MesloLGM NF', 'agave Nerd Font', 'Noto Emoji','FontAwesome',  'Droid Sans Mono', 'monospace', monospace",
  "editor.minimap.enabled": false,
  "editor.hover.enabled": false,
  //
  "search.exclude": {
    "build/": true,
    "dist": true,
    "node_modules": true,
    "**/node_modules": true,
    "package-lock.json": true,
    "*.d.ts": true
  },
  // Highlight matching tag
  "highlight-matching-tag.enabled": true,
  "highlight-matching-tag.highlightFromAttributes": true,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.highlightFromName": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  "highlight-matching-tag.noDefaultEmptyElements": true,
  "highlight-matching-tag.showPath": true,
  "highlight-matching-tag.showRuler": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "left": {
        "custom": {
          "borderWidth": "3px 0 3px 3px",
          "borderStyle": "solid",
          "borderColor": "#D28520",
          "borderRadius": "1px"
        }
      },
      "right": {
        "custom": {
          "borderWidth": "3px 3px 3px 0",
          "borderStyle": "solid",
          "borderColor": "#D28520",
          "borderRadius": "1px"
        }
      }
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "beginkeyword",
    "castwide",
    "catppuccin",
    "Cocon",
    "codetag",
    "Colour",
    "cssentity",
    "esbenp",
    "hafez",
    "Hasubi",
    "HMTL",
    "javadoc",
    "Meslo",
    "neovim",
    "Nord",
    "Noto",
    "rainbowend",
    "readwrite",
    "redish",
    "selectionset",
    "setext",
    "solargraph",
    "todohighlight"
  ],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "*.erb": "html",
    "ruby": "html"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "solargraph.transport": "stdio",
  "solargraph.formatting": true,
  "solargraph.diagnostics": true,
  "solargraph.commandPath": "/home/hafez/.asdf/shims/solargraph",
  // "[ruby]": {
  //   "editor.defaultFormatter": "castwide.solargraph"
  // },
  "diffEditor.codeLens": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 2,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.sideBar.location": "right",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.compactFolders": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "catppuccin-mocha",
  "auto-close-tag.insertSpaceBeforeSelfClosingTag": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "terminal.integrated.tabFocusMode": false,
  "window.zoomLevel": 1,
  // "vitest.commandLine": "vitest",
  "workbench.editor.openSideBySideDirection": "down",
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "color": "#1e1e2e",
      "backgroundColor": "#fab387",
      "isWholeLine": true
    },
    {
      "text": "Question:",
      "color": "#1e1e2e",
      "backgroundColor": "#f38ba8",
      "isWholeLine": true
    }
  ],
  "editor.codeLensFontFamily": "'Hasubi Mono', 'MesloLGM NF'",
  "editor.inlayHints.fontFamily": "'Hasubi Mono', 'MesloLGM NF'",
  "terminal.integrated.fontFamily": "'Hasubi Mono', 'SpaceMono Nerd Font Mono'",
  "workbench.colorCustomizations": {
    "[Catppuccin Mocha]": {
      "sideBarSectionHeader.background": "#89dceb",
      "icon.foreground": "#f38ba8",
      "sideBarSectionHeader.foreground": "#1e1e2e",
    }
  },
  "gitlens.heatmap.coldColor": "#f38ba8",
  "gitlens.heatmap.hotColor": "#f38ba8",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "Question",
    "[ ]",
    "[x]"
  ],
  "todo-tree.general.statusBar": "current file",
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#1e1e2e",
      "type": "text",
      "iconColour": "#89dceb",
      "gutterIcon": true
    },
    "FIXME": {
      "icon": "beaker",
      "iconColour": "#89dceb"
    },
    "Question": {
      "foreground": "#1e1e2e",
      "iconColour": "#89dceb",
      "icon": "question"
    }
  },
  "workbench.list.openMode": "singleClick",
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "python.defaultInterpreterPath": "/bin/python",
  "liveshare.accessibility.soundsEnabled": false,
  "liveshare.accessibility.voiceEnabled": false,
  "editor.accessibilitySupport": "on"
}